package ncg.chimpsahoy.everdark;
import java.io.*;

public class Main {
	public static void main(String[] args) {
		Player p = Player.characterCreation();
		try {
		p.saveToFile();
		} catch (FileNotFoundException fnfe) {
			System.out.println(fnfe.getMessage());
			fnfe.printStackTrace();
		}
	}

}
